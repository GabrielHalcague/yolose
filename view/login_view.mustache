{{> header}}
<section class="container">
    <h2 class="mb-4 text-center">Formulario de ingreso</h2>
    <article class="p-5 bg-dark-subtle rounded">
        <form class="d-flex justify-content-center form-yolose" action="index.php?module=login&action=iniciarSesion" method="post" enctype="multipart/form-data">
            <div class="w-50">
                <label for="username" class="form-label">Usuario</label>
                <input type="text" class="form-control" name="username" id="username" />
                <label for="password" class="form-label">Password</label>
                <input type="password" class="form-control" name="password" id="password" />
                <div class="mt-3 text-end">
                    <button type="submit" class="btn btn-yolose px-5">Enviar</button>
                </div>
            </div>
        </form>
    </article>
</section>
{{> footer}}

